VAR x: int;

FUNCTION main(){

    IF(1>2){
        x = 1;
    }
    IF(3 >= 5){
        x = 2;
    }
    IF(1 < 2){
        x = 3;
    }
    IF(3<=4){
        x = 4;
    }

    IF(5!=6){
        x = 5;
    }
    IF(!0){
        x = 6;
    }
    IF(1 == 1 && 2 != 3){
        x = 7;
    }
    IF(1&2){
        x = 8;
    }
}